{
    "type": "postgres",
    "host": "localhost",
    "port": 5432,
    "username": "postgres",
    "password": "postgres",
    "database": "magna_farm",
    "options": {
      "useUTC": false
    },

    "synchronize": false,
    "logging": false,

    "entities"   : [
      "dist/models/*.js"
   ],
   "migrations" : [
      "dist/migration/**/*.js"
   ],
   "subscribers": [
      "dist/subscriber/**/*.js"
   ],
    "cli": {
        "migrationsDir": "src/database/migrations"
    }
 }
